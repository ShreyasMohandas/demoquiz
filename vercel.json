{
    "version": 2,
    "builds": [
      {
        "src": "modelrev2/wsgi.py",
        "use": "@vercel/python",
        "config": {
          "maxLambdaSize": "15mb",
          "runtime": "python3.11"
        }
      }
    ],
    "routes": [
      {
        "src": "/static/(.*)",
        "dest": "/static/$1"
      },
      {
        "src": "/(.*)",
        "dest": "modelrev2/wsgi.py"
      }
    ]
  }
  